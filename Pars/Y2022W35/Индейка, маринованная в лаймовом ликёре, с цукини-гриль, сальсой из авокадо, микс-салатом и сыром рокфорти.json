3d\u0430 \u043f\u0440\u043e\u0442\u0438\u0432\u0435\u043d\u044c, \u0441\u043a\u043e\u0432\u043e\u0440\u043e\u0434\u0443 \u0441\u043e\u0445\u0440\u0430\u043d\u0438\u0442\u0435.", "1": "\u041f\u043e\u0441\u0442\u0430\u0432\u044c\u0442\u0435 \u0438\u043d\u0434\u0435\u0439\u043a\u0443 \u0437\u0430\u043f\u0435\u043a\u0430\u0442\u044c\u0441\u044f \u0432 \u0440\u0430\u0437\u043e\u0433\u0440\u0435\u0442\u0443\u044e \u0434\u0443\u0445\u043e\u0432\u043a\u0443 \u043d\u0430 12 \u043c\u0438\u043d\u0443\u0442. \u0414\u043e\u0441\u0442\u0430\u043d\u044c\u0442\u0435 \u043f\u0440\u043e\u0442\u0438\u0432\u0435\u043d\u044c \u0438\u0437 \u0434\u0443\u0445\u043e\u0432\u043a\u0438, \u0438\u043d\u0434\u0435\u0439\u043a\u0443 \u043f\u0435\u0440\u0435\u043b\u043e\u0436\u0438\u0442\u0435 \u043d\u0430 \u0434\u043e\u0441\u043a\u0443 \u0438 \u043d\u0430\u0440\u0435\u0436\u044c\u0442\u0435 \u0442\u043e\u043d\u043a\u0438\u043c\u0438 \u043b\u043e\u043c\u0442\u0438\u043a\u0430\u043c\u0438.", "2": "\u041b\u0438\u0441\u0442\u043e\u0447\u043a\u0438 \u043f\u0435\u0442\u0440\u0443\u0448\u043a\u0438 \u0438 \u043a\u0438\u043d\u0437\u044b \u043e\u0442\u0434\u0435\u043b\u0438\u0442\u0435 \u043e\u0442 \u0441\u0442\u0435\u0431\u043b\u0435\u0439 \u0438 \u043d\u0430\u0440\u0443\u0431\u0438\u0442\u0435 \u043c\u0435\u043b\u043a\u043e. \u041f\u0435\u0440\u0435\u0446 \u0447\u0438\u043b\u0438 \u0440\u0430\u0437\u0440\u0435\u0436\u044c\u0442\u0435 \u0432\u0434\u043e\u043b\u044c, \u043e\u0441\u0432\u043e\u0431\u043e\u0434\u0438\u0442\u0435 \u043e\u0442 \u0441\u0435\u043c\u044f\u043d, \u043e\u0442\u0440\u0435\u0436\u044c\u0442\u0435 2 \u0442\u043e\u043d\u043a\u0438\u0445 \u043f\u043b\u0430\u0441\u0442\u0438\u043d\u043a\u0438 (\u0438\u043b\u0438 \u0431\u043e\u043b\u044c\u0448\u0435 \u0434\u043b\u044f \u0431\u043e\u043b\u044c\u0448\u0435\u0439 \u043e\u0441\u0442\u0440\u043e\u0442\u044b) \u0438 \u0438\u0437\u043c\u0435\u043b\u044c\u0447\u0438\u0442\u0435 \u0438\u0445. \u0412\u0438\u043d\u043e\u0433\u0440\u0430\u0434 \u043d\u0430\u0440\u0435\u0436\u044c\u0442\u0435 \u043c\u0435\u043b\u043a\u0438\u043c \u043a\u0443\u0431\u0438\u043a\u043e\u043c.", "3": "\u0423 \u043f\u043e\u043c\u0438\u0434\u043e\u0440\u0430 \u0443\u0434\u0430\u043b\u0438\u0442\u0435 \u043f\u043b\u043e\u0434\u043e\u043d\u043e\u0436\u043a\u0443, \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0443 \u043f\u043e\u043c\u0438\u0434\u043e\u0440\u0430 \u043d\u0430\u0440\u0435\u0436\u044c\u0442\u0435 \u043c\u0435\u043b\u043a\u0438\u043c \u043a\u0443\u0431\u0438\u043a\u043e\u043c, \u043e\u0441\u0442\u0430\u0432\u0448\u0438\u0439\u0441\u044f - \u0442\u043e\u043d\u043a\u0438\u043c\u0438 \u0434\u043e\u043b\u044c\u043a\u0430\u043c\u0438. \u041b\u0438\u0441\u0442\u044c\u044f \u0441\u0430\u043b\u0430\u0442\u0430 \u0430\u0439\u0441\u0431\u0435\u0440\u0433, \u0440\u043e\u043c\u0430\u043d\u043e \u0438 \u0440\u0443\u043a\u043a\u043e\u043b\u0443 \u043d\u0430\u0440\u0432\u0438\u0442\u0435 \u0440\u0443\u043a\u0430\u043c\u0438 \u043a\u0440\u0443\u043f\u043d\u043e \u0432 \u043c\u0438\u0441\u043a\u0443, \u0437\u0430\u0442\u0435\u043c \u0434\u043e\u0431\u0430\u0432\u044c\u0442\u0435 \u0434\u043e\u043b\u044c\u043a\u0438 \u043f\u043e\u043c\u0438\u0434\u043e\u0440\u0430.", "4": "\u0423 \u0430\u0432\u043e\u043a\u0430\u0434\u043e \u0443\u0434\u0430\u043b\u0438\u0442\u0435 \u043a\u043e\u0441\u0442\u043e\u0447\u043a\u0443, \u043c\u044f\u043a\u043e\u0442\u044c \u043e\u0447\u0438\u0441\u0442\u0438\u0442\u0435 \u043e\u0442 \u043a\u043e\u0436\u0443\u0440\u044b, \u043f\u0435\u0440\u0435\u043b\u043e\u0436\u0438\u0442\u0435 \u0432 \u0447\u0438\u0441\u0442\u0443\u044e \u043c\u0438\u0441\u043a\u0443 \u0438 \u0440\u0430\u0437\u043e\u043c\u043d\u0438\u0442\u0435 \u0432\u0438\u043b\u043a\u043e\u0439. \u0414\u043e\u0431\u0430\u0432\u044c\u0442\u0435 \u043d\u0430\u0440\u0435\u0437\u0430\u043d\u043d\u044b\u0435 \u043a\u0443\u0431\u0438\u043a\u043e\u043c \u043f\u043e\u043c\u0438\u0434\u043e\u0440 \u0438 \u0432\u0438\u043d\u043e\u0433\u0440\u0430\u0434, \u043f\u0435\u0440\u0435\u0446 \u0447\u0438\u043b\u0438, \u043d\u0430\u0440\u0443\u0431\u043b\u0435\u043d\u043d\u044b\u0435 \u043a\u0438\u043d\u0437\u0443 \u0438 \u043f\u0435\u0442\u0440\u0443\u0448\u043a\u0443, \u0432\u043b\u0435\u0439\u0442\u0435 \u0441\u043e\u043a \u043b\u0430\u0439\u043c\u0430, \u0440\u0430\u0441\u043a\u0440\u043e\u0448\u0438\u0442\u0435 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0443 \u0441\u044b\u0440\u0430 \u0440\u043e\u043a\u0444\u043e\u0440\u0442\u0438, \u043f\u043e\u0441\u043e\u043b\u0438\u0442\u0435 \u0438 \u0442\u0449\u0430\u0442\u0435\u043b\u044c\u043d\u043e \u043f\u0435\u0440\u0435\u043c\u0435\u0448\u0430\u0439\u0442\u0435.", "5": "\u0426\u0443\u043a\u0438\u043d\u0438 \u0440\u0430\u0437\u0440\u0435\u0436\u044c\u0442\u0435 \u0432\u0434\u043e\u043b\u044c \u043f\u043e\u043f\u043e\u043b\u0430\u043c, \u0437\u0430\u0442\u0435\u043c \u043a\u0430\u0436\u0434\u0443\u044e \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u043a\u0443 \u0440\u0430\u0437\u0440\u0435\u0436\u044c\u0442\u0435 \u043d\u0430 3 \u0447\u0430\u0441\u0442\u0438. \u0421\u043a\u043e\u0432\u043e\u0440\u043e\u0434\u0443-\u0433\u0440\u0438\u043b\u044c, \u0432 \u043a\u043e\u0442\u043e\u0440\u043e\u0439 \u0436\u0430\u0440\u0438\u043b\u0430\u0441\u044c \u0438\u043d\u0434\u0435\u0439\u043a\u0430, \u0441\u043c\u0430\u0436\u044c\u0442\u0435 \u0434\u0432\u0443\u043c\u044f \u043a\u0430\u043f\u043b\u044f\u043c\u0438 \u043e\u043b\u0438\u0432\u043a\u043e\u0432\u043e\u0433\u043e \u043c\u0430\u0441\u043b\u0430 \u0438 \u043e\u0431\u0436\u0430\u0440\u044c\u0442\u0435 \u0446\u0443\u043a\u0438\u043d\u0438 30 \u0441\u0435\u043a\u0443\u043d\u0434 \u043d\u0430 \u0441\u0438\u043b\u044c\u043d\u043e\u043c \u043e\u0433\u043d\u0435 \u0441 \u043e\u0434\u043d\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b, \u043f\u0435\u0440\u0435\u0432\u0435\u0440\u043d\u0438\u0442\u0435 \u0438 \u0436\u0430\u0440\u044c\u0442\u0435 \u0435\u0449\u0451 30 \u0441\u0435\u043a\u0443\u043d\u0434 \u0441 \u0434\u0440\u0443\u0433\u043e\u0439 \u0441\u0442\u043e\u0440\u043e\u043d\u044b. \u0421\u043d\u0438\u043c\u0438\u0442\u0435 \u0441 \u043e\u0433\u043d\u044f, \u0446\u0443\u043a\u0438\u043d\u0438 \u043f\u0435\u0440\u0435\u043b\u043e\u0436\u0438\u0442\u0435 \u0432 \u043c\u0438\u0441\u043a\u0443 \u043a \u0441\u0430\u043b\u0430\u0442\u0443. \u0414\u043e\u0431\u0430\u0432\u044c\u0442\u0435 \u043f\u043e\u043b\u043e\u0432\u0438\u043d\u0443 \u0441\u0430\u043b\u044c\u0441\u044b \u0438\u0437 \u0430\u0432\u043e\u043a\u0430\u0434\u043e, \u0432\u043b\u0435\u0439\u0442\u0435 1 \u0441\u0442. \u043b\u043e\u0436\u043a\u0443 \u043e\u043b\u0438\u0432\u043a\u043e\u0432\u043e\u0433\u043e \u043c\u0430\u0441\u043b\u0430 \u0438 \u043f\u0435\u0440\u0435\u043c\u0435\u0448\u0430\u0439\u0442\u0435.", "6": "\u0420\u0430\u0437\u043b\u043e\u0436\u0438\u0442\u0435 \u0441\u0430\u043b\u0430\u0442 \u043f\u043e \u0442\u0430\u0440\u0435\u043b\u043a\u0430\u043c, \u0441\u0432\u0435\u0440\u0445\u0443 \u0432\u044b\u043b\u043e\u0436\u0438\u0442\u0435 \u043b\u043e\u043c\u0442\u0438\u043a\u0438 \u0438\u043d\u0434\u0435\u0439\u043a\u0438, \u0440\u0430\u0441\u043a\u0440\u043e\u0448\u0438\u0442\u0435 \u043e\u0441\u0442\u0430\u0432\u0448\u0438\u0439\u0441\u044f \u0441\u044b\u0440 \u0438 \u043f\u043e\u0434\u0430\u0432\u0430\u0439\u0442\u0435 \u0441 \u043e\u0441\u0442\u0430\u0432\u0448\u0435\u0439\u0441\u044f \u0441\u0430\u043b\u044c\u0441\u043e\u0439 \u0438\u0437 \u0430\u0432\u043e\u043a\u0430\u0434\u043e \u0438 \u0445\u0440\u0443\u0441\u0442\u044f\u0449\u0438\u043c\u0438 \u043a\u0440\u0435\u043a\u0435\u0440\u0430\u043c\u0438."}}